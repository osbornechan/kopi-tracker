<h2>Kopis</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Roast</th>
      <th scope="col">Origin</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
  	<% @kopis.each do |kopi| %>
	    <tr>
	      <td><%= kopi.name %></td>
	      <td><%= kopi.roast.kind %></td>
	      <td><%= kopi.origin.location %></td>
	      <td><%= kopi.price %></td>
	    </tr>
	   <% end %>
  </tbody>